{
    "name": "actual/laravel-intl",
    "description": "Easy to use internationalization functions for Laravel",
    "keywords": [
        "laravel",
        "internationalization",
        "intl",
        "i18n",
        "l10n"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Propaganistas",
            "email": "Propaganistas@users.noreply.github.com"
        },
        {
            "name": "Actual Digital",
            "email": "actualci@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=7.1.3|^8.0",
        "illuminate/events": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "nesbot/carbon": "^3.0",
        "commerceguys/intl": "^2.0",
        "umpirsky/country-list": "^2.0",
        "umpirsky/locale-list": "^1.0",
        "punic/punic": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Propaganistas\\LaravelIntl\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Propaganistas\\LaravelIntl\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Propaganistas\\LaravelIntl\\IntlServiceProvider"
            ]
        }
    }
}
